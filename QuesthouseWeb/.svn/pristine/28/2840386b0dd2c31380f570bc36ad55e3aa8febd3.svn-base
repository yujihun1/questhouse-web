@model IEnumerable<QuesthouseWeb.Models.Test>


<h2>게시글 목록</h2>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>제목</th>
            <th>내용</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var data in Model)
        {
            <tr>
                <td>@data.Id</td>
                <td>@data.Name</td>
                <td>@data.Age</td>
            </tr>
        }
    </tbody>
</table>